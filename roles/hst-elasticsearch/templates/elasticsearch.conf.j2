[program:{{ elastic_name }}]
command={{ elastic_folder }}/{{ elastic_name }}/bin/elasticsearch
socket=tcp://{{ loopback_adress }}:{{ elastic_port }}
autostart=true
autorestart=true
startsecs=5
stopwaitsecs=5
stopasgroup=true
killasgroup=true
environment=_JAVA_OPTIONS="-Djava.io.tmpdir={{ elastic_tmpdir }} -XX:-UsePerfData",JAVA_HOME="{{ java_home }} -Dlog4j2.formatMsgNoLookups=true",ES_HEAP_SIZE="{{ elastic_memory }}",ES_TMPDIR="{{ elastic_tmpdir }}"
