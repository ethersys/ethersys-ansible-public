[program:{{ redis_name }}]
command=/home/{{ ansible_user }}/redis/{{ redis_name }}/redis-{{ redis_version }}/src/redis-server /home/{{ ansible_user }}/redis/{{ redis_name }}/etc/redis.conf 
socket=tcp://{{ loopback_adress }}:{{ redis_port }}
autostart=true
autorestart=true
startsecs=5
stopwaitsecs=5
stopasgroup=true
killasgroup=true
